<ul>Name : @ViewData["Name"]</ul>
<ul>Age : @ViewData["Age"]</ul>
<ul>Single : @ViewData["Single"]</ul>

@{
    foreach(var item in ViewData)
            {
                <li>@item.Key, @item.Value</li>
            }
}